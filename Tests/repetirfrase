}


string SolicitarFrase(){

	std::cout << "Ingrese frase a repetir \n";
	string frasee;
	std::cin >> frasee;
	return frasee;

}

void RepetirFrase(string a){

	int cont = 0;

	if (cont <= 100){

		std::cout a;
		cont = cont + 1;
	}
	else{

		return 0;
	}


}
