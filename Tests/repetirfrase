using std::string;

int main(){

	
	string SolicitarFrase();
	string Frase = SolicitarFrase();
	void RepetirFrase (string);
	RepetirFrase (Frase);
}


string SolicitarFrase(){

	std::cout << "Ingrese frase a repetir \n";
	string frasee;
	std::cin >> frasee;
	return frasee;

}

void RepetirFrase(string a){

	
	for (int i = 0; i < 10; ++i){
		
		std::cout << a << "\n";
	}
	
}
