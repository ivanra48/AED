#include <iostream>
#include <string>

using std::string;

int main(){

	
	string SolicitarFrase();
	string Frase = SolicitarFrase();
	void RepetirFrase (string);
	RepetirFrase (Frase);
}


string SolicitarFrase(){

	std::cout << "Ingrese frase a repetir \n";
	string frasee;
	std::cin >> frasee;
	return frasee;

}

void RepetirFrase(string a){

	int cont = 0;

	if (cont < 100){

		std::cout << a;
		cont = cont + 1;
	}
	
}
